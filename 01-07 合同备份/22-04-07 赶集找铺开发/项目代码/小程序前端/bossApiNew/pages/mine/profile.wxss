.mine-info-container {
    padding-bottom: 10rpx;
}

.mine-info-container .page_ad .header_bg {
    background-size: cover;
    height: 480rpx;
}

.mine-info-container .page_ad .header_bg .header_title {
    color: #333;
    font-size: 16px;
    font-weight: 500;
    text-align: center;
}

.mine-info-container .page_ad .header_bg .header_info {
    align-items: center;
    display: flex;
    justify-content: space-between;
    padding: 0 30rpx 30rpx;
}

.mine-info-container .page_ad .header_bg .header_info .head_img {
    background-size: cover;
    border-radius: 27px;
    height: 54px;
    overflow: hidden;
    width: 54px;
}

.mine-info-container .page_ad .header_bg .header_info .nick_name {
    padding: 24rpx;
}

.mine-info-container .page_ad .header_bg .header_info .nick_name .usernickname {
    color: #333;
    font-size: 32rpx;
}

.mine-info-container .page_ad .header_bg .header_info .nick_name .nickphone {
    color: #333;
    font-size: 24rpx;
    margin-top: 20rpx;
}

.mine-info-container .page_ad .header_bg .data_info {
    display: flex;
}

.mine-info-container .page_ad .header_bg .data_info view {
    color: #333;
    display: flex;
    flex: 1;
    flex-direction: column;
}

.mine-info-container .page_ad .header_bg .data_info view text {
    text-align: center;
}

.mine-info-container .page_ad .header_bg .dashboard_item {
    align-items: center;
    display: flex;
    flex: 1;
    flex-direction: column;
}

.mine-info-container .page_ad .header_bg .dashboard_item .num {
    color: #333;
    font-size: 32rpx;
    font-weight: 500;
}

.mine-info-container .page_ad .header_bg .dashboard_item .msg {
    border-right: 1rpx solid #999;
    color: #333;
    font-size: 22rpx;
    margin-top: 10rpx;
    text-align: center;
    width: 100%;
}

.mine-info-container .page_ad .header_bg .dashboard_item:last-child .msg {
    border-right: none;
}

.mine-info-container .page_ad .header_bg .openMember {
    color: #666;
    display: flex;
    margin-left: auto;
    padding: 26rpx 0;
}

.mine-info-container .page_ad .header_bg .openMember.right_icon:after {
    margin-top: 10rpx;
}

.mine-info-container .page_ad .memberBox {
    background-color: #fff;
    border-radius: 10px;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    margin: -15px 30rpx 0;
    padding: 30rpx;
}

.mine-info-container .page_ad .memberBox view {
    align-items: center;
    display: flex;
    flex: 1;
    flex-direction: column;
}

.mine-info-container .page_ad .memberBox view text {
    padding-top: 10px;
}

.mine-info-container .page_ad .home_bg {
    background-size: cover;
    border-radius: 30rpx;
    height: 100%;
    margin: 0 10px;
}

.mine-info-container .header {
    background-color: #2facfb;
    position: fixed;
    width: 100%;
    z-index: 100;
}

.mine-info-container .header .header-img {
    align-items: center;
    display: flex;
    height: 44px;
    padding-left: 10px;
    padding-top: 20px;
}

.mine-info-container .detail {
    background-color: #fff;
    margin-top: 24rpx;
}

.mine-info-container .detail .time_chart {
    padding: 40rpx 40rpx 20rpx;
}

.mine-info-container .detail .time_chart image {
    width: 100%;
}

.mine-info-container .detail .detail_body {
    color: #666;
    line-height: 1.3;
    word-break: break-all;
}

.mine-info-container .pannel-body {
    padding: 0;
}

.mine-info-container .mine_pannel {
    background-color: #fff;
    border-radius: 10px;
    padding: 0 24rpx;
}

.mine-info-container .mine_pannel .column .title {
    align-items: center;
    border-bottom: 1px solid hsla(0,0%,89%,.38);
    display: flex;
    justify-content: space-between;
    line-height: 42rpx;
    padding: 30rpx 20rpx;
}

.mine-info-container .mine_pannel .column .title text {
    line-height: 24px;
}

.mine-info-container .mine_pannel .column .tips {
    align-items: center;
    color: #ff9543;
    display: flex;
    font-size: 24rpx;
}

.mine-info-container .mine_pannel .column .share_btn {
    border: none;
    font-size: 28rpx;
    line-height: 23px;
    margin-left: 0;
    padding: 0;
    text-align: left;
    width: 100%;
}