.order-list-container .nav {
    align-items: center;
    background-color: #fff;
    border-bottom: 1px solid hsla(0,0%,89%,.38);
    display: flex;
    height: 88rpx;
    justify-content: center;
    line-height: 88rpx;
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 10;
}

.order-list-container .nav .type {
    flex: 1;
    padding: 10px;
    text-align: center;
}

.order-list-container .nav .type .active {
    border-bottom: 2px solid #2facfc;
    color: #2facfc;
}

.order-list-container .add_concat {
    align-items: center;
    background-color: #fff;
    border-radius: 10rpx;
    display: flex;
    font-size: 28rpx;
    justify-content: space-between;
    margin: 24rpx;
    padding: 40rpx 30rpx;
}

.order-list-container .goods_list {
    margin-bottom: 100px;
    padding-bottom: 100px;
    padding-top: 0;
}

.order-list-container .button_submit {
    background: linear-gradient(90deg,#f79204,#f75b4c);
    bottom: 0;
    color: #fff;
    font-size: 36rpx;
    line-height: 92rpx;
    margin: 100rpx 30rpx 20px;
    position: fixed;
    width: calc(100vw - 60rpx);
}

.order-list-container .button_submit::after {
    border: none;
}