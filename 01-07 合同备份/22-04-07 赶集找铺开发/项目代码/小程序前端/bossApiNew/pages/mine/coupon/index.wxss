.main-title {
    font-size: 32rpx;
}

.pannel .pannel-title {
    font-weight: 500;
    padding: 24rpx;
}

.pannel .pannel-body {
    padding: 24rpx;
}

view {
    color: #333;
    font-size: 28rpx;
    line-height: 1;
}

.right_icon:after {
    background-image: url("https://tystatic.liuxx.com/wechat/images/right_icon.png");
    background-size: cover;
    content: " ";
    display: block;
    height: 20rpx;
    margin-left: 12rpx;
    width: 20rpx;
}

.empty-content {
    margin: 1.59rem .3rem 1rem;
    position: relative;
    text-align: center;
}

.empty-content .empty-card .er-code-status {
    height: 128rpx;
    margin: 300rpx 0 0;
    width: 128rpx;
}

.empty-content .empty-card .empty-no {
    display: inline-block;
    margin: 40rpx 0;
    width: 100%;
}

.empty-content .empty-card .empty-buy {
    color: #999;
    font-size: 32rpx;
    padding: .8rem 0;
}

.coupon-cell-container {
    margin: 40rpx 20rpx;
}

.coupon-cell-container .item-coupon-cell-container {
    background: radial-gradient(circle at right top,transparent 20rpx,#f75b4c 0) 0 0 /160rpx 51% no-repeat,radial-gradient(circle at right bottom,transparent 20rpx,#f75b4c 0) 0 100% /160rpx 51% no-repeat,radial-gradient(circle at left top,transparent 20rpx,#fff 0) 100% 0 /calc(100vw - 160rpx - 40rpx) 51% no-repeat,radial-gradient(circle at left bottom,transparent 20rpx,#fff 0) 100% 100% /calc(100vw - 160rpx - 40rpx) 51% no-repeat;
    border-radius: 10rpx;
    display: flex;
    height: 160rpx;
    overflow: hidden;
    position: relative;
}

.coupon-cell-container .item-coupon-cell-container .item-coupon-left {
    align-items: center;
    display: flex;
    flex-direction: column;
    height: 100%;
    justify-content: center;
    width: 160rpx;
}

.coupon-cell-container .item-coupon-cell-container .item-coupon-right {
    align-items: center;
    display: flex;
    flex-direction: row;
    height: 100%;
    padding: 0 12rpx;
    width: calc(100vw - 160rpx - 40rpx);
}

.coupon-cell-container .item-coupon-cell-container .item-coupon-right .item-coupon-status {
    align-items: flex-end;
    background-color: #f75b4c;
    display: flex;
    height: 120rpx;
    justify-content: center;
    position: absolute;
    right: -60rpx;
    top: -60rpx;
    transform: rotate(45deg);
    width: 120rpx;
}

