.order-list-container .nav {
    align-items: center;
    background-color: #fff;
    border-bottom: 1px solid hsla(0,0%,89%,.38);
    display: flex;
    height: 88rpx;
    justify-content: center;
    line-height: 88rpx;
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 10;
}

.order-list-container .nav .type {
    flex: 1;
    padding: 10px;
    text-align: center;
}

.order-list-container .nav .type .active {
    border-bottom: 2px solid #ffe60f;
    color: #ffe60f;
}

.order-list-container .goods_list {
    margin-top: 88rpx;
    padding: 24rpx 24rpx 100px;
}

.order-list-container .goods_list .item {
    flex-direction: column;
    margin: 20rpx;
}

.order-list-container .goods_list .item .tag {
    font-size: 28rpx;
}

.order-list-container .goods_list .item .tag text {
    border: none;
    padding: 0;
}

.order-list-container .goods_list .item .pannel_title {
    border-bottom: 2rpx solid #f6f6f6;
    color: #999;
    display: flex;
    font-size: 24rpx;
    justify-content: space-between;
    line-height: 30rpx;
    margin: 10rpx 30rpx 0;
    padding-bottom: 30rpx;
}

.order-list-container .goods_list .item .pannel_title .item_order_no_tag {
    width: 80rpx;
}

.order-list-container .goods_list .item .pannel_title .item_order_no {
    word-wrap: break-word;
    width: 440rpx;
    word-break: keep-all;
}

.order-list-container .goods_list .item .pannel_title .item_order_status {
    text-align: right;
    width: 100rpx;
}

.order-list-container .goods_list .item .pannel_body {
    display: flex;
    margin: 30rpx;
}

.order-list-container .goods_list .item .pannel_body .item_order_title {
    color: #333;
    font-size: 28rpx;
}

.order-list-container .goods_list .item .pannel_foot {
    align-items: center;
    border-top: 2rpx solid #f6f6f6;
    display: flex;
    justify-content: space-between;
    margin: 0 30rpx;
}

.order-list-container .goods_list .item .pannel_foot .btn_area {
    display: flex;
    justify-content: flex-end;
}

.order-list-container .goods_list .item .pannel_foot .btn_area text {
    border: 2rpx solid #999898;
    border-radius: 30rpx;
    color: #999898;
    margin: 20rpx 0 0 10rpx;
    padding: 10rpx;
    text-align: center;
    width: 80px;
}

.order-list-container .button_submit {
    background-color: #ffe60f;
    bottom: 0;
    color: #fff;
    font-size: 36rpx;
    line-height: 92rpx;
    margin: 100rpx 30rpx 20px;
    position: fixed;
    width: calc(100vw - 60rpx);
}

.order-list-container .button_submit::after {
    border: none;
}