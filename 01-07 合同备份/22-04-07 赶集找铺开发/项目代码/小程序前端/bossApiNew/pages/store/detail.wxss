.main-title {
    font-size: 32rpx;
}

.pannel .pannel-title {
    font-weight: 500;
    padding: 24rpx;
}

.pannel .pannel-body {
    padding: 24rpx;
}

view {
    color: #333;
    font-size: 28rpx;
    line-height: 1;
}

.right_icon:after {
    background-image: url("https://tystatic.liuxx.com/wechat/images/right_icon.png");
    background-size: cover;
    content: " ";
    display: block;
    height: 20rpx;
    margin-left: 12rpx;
    width: 20rpx;
}

.tour-item {
    background: #fff;
    display: flex;
    padding: 24rpx;
}

.tour-item .thumb {
    background-size: cover;
    border-radius: 12rpx;
    height: 200rpx;
    width: 200rpx;
}

.tour-item .thumb .bread {
    background: linear-gradient(90deg,#fe4028,#ff5a3d);
    border-radius: 12rpx 0 12rpx 0;
    color: #fff;
    font-size: 24rpx;
    padding: 6rpx 12rpx;
    width: 50rpx;
}

.tour-item .info {
    flex: 1;
    flex-direction: column;
    overflow: hidden;
    padding-left: 24rpx;
    position: relative;
}

.tour-item .info,.tour-item .info .title {
    display: flex;
    justify-content: space-between;
}

.tour-item .info .title {
    line-height: 20px;
    padding: 0 0 12rpx;
}

.tour-item .info .title .tag {
    background-color: rgba(255,230,15,.2);
    border-radius: 5px;
    color: #ffe60f;
    font-size: 10px;
    font-weight: 400;
    padding: 0 5px;
}

.tour-item .info .title text {
    font-size: 28rpx;
    font-weight: 700;
}

.tour-item .info .info-des {
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    color: #666;
    display: -webkit-box;
    font-size: 24rpx;
    margin-bottom: 12rpx;
    max-height: 48rpx;
    overflow: hidden;
}

.tour-item .info .info-tags {
    display: flex;
    margin-bottom: 12rpx;
}

.tour-item .info .info-tags text {
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    border: 1rpx solid #f2f3f4;
    border-radius: 6rpx;
    color: #666;
    display: -webkit-box;
    font-size: 22rpx;
    margin-right: 12rpx;
    overflow: hidden;
    padding: 6rpx;
}

.tour-item .info .label {
    align-items: center;
    display: flex;
    justify-content: space-between;
}

.tour-item .info .label .chat {
    background-color: #ffe60f;
    color: #fff!important;
    padding: 2px;
}

.tour-item .info .label .price {
    color: #fd4c38;
    font-size: 24rpx;
}

.tour-item .info .label .price .money {
    font-size: 40rpx;
    font-weight: 500;
    margin-right: 10rpx;
}

.tour-item .info .label .label-des {
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    color: #ffe60f;
    display: -webkit-box;
    flex: 1;
    font-size: 22rpx;
    height: 22rpx;
    overflow: hidden;
}

.tour-item .info .label .label-distance {
    color: #999;
    font-size: 22rpx;
    padding-right: .2rem;
    text-align: right;
}

.tour-bottom {
    align-items: center;
    background: #fff;
    justify-content: space-between;
}

.btn-area,.tour-bottom {
    display: flex;
    padding: 0 24rpx 24rpx;
}

.btn-area {
    justify-content: flex-end;
}

.btn-area text {
    border: 1rpx solid #ff9543;
    border-radius: 24rpx;
    color: #ff9543;
    font-size: 12px;
    margin-left: 24rpx;
    padding: 12rpx 24rpx;
}

.custom-header {
    position: fixed;
    width: 100%;
    z-index: 100;
}

.custom-header .header-img {
    align-items: center;
    display: flex;
    height: 44px;
    padding-top: 20px;
}

.custom-header .header-img .icon {
    border-radius: 20px;
    height: 40rpx;
    margin-left: 10px;
    width: 40rpx;
}

.custom-header .header_title {
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    display: -webkit-box;
    flex: 1;
    font-size: 18px;
    font-weight: 500;
    overflow: hidden;
    text-align: center;
    text-overflow: ellipsis;
    word-break: break-all;
}

page {
    background-color: #f2f3f4;
}

.scenic-list-container {
    height: calc(100vh);
}

.scenic-list-container .banner {
    height: 550rpx;
    position: relative;
    width: 100%;
}

.scenic-list-container .banner .swiper_banner {
    height: 550rpx;
    position: absolute;
    top: 0;
    width: 100%;
}

.scenic-list-container .banner .swiper_banner .scenic_cover {
    height: 550rpx;
    width: 100%;
}

.scenic-list-container .content {
    padding-bottom: 230rpx;
}

.scenic-list-container .content .info-view {
    background-color: #fff;
    bottom: 24rpx;
    box-sizing: border-box;
    display: flex;
    flex-flow: column;
    padding: 24rpx;
}

.scenic-list-container .content .info-view .main_title {
    flex: 1;
    font-weight: 700;
    line-height: 40rpx;
}

.scenic-list-container .content .info-view .main_title .tip {
    background: #f75b4c;
    border-radius: 8rpx;
    color: #fff;
    font-size: 24rpx;
    margin-right: 10rpx;
    padding: 0 4px;
}

.scenic-list-container .content .info-view .btn_view {
    align-items: center;
    color: #fdbc17;
    display: flex;
    flex-direction: column;
    justify-content: center;
}

.scenic-list-container .content .info-view .btn_view .primary_text {
    font-size: 24rpx;
    padding-top: 10rpx;
}

.scenic-list-container .content .info-view .tag {
    display: flex;
    margin: 24rpx 0;
}

.scenic-list-container .content .info-view .tag text {
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    border: 1rpx solid #f2f3f4;
    border-radius: 6rpx;
    color: #666;
    display: -webkit-box;
    margin-right: 16rpx;
    overflow: hidden;
    padding: 6rpx;
}

.scenic-list-container .content .info-view .price {
    border-bottom: 1rpx solid #e4e4e4;
    border-top: 1rpx solid #e4e4e4;
    display: flex;
    margin: 12rpx 0;
    padding: 12rpx 0;
}

.scenic-list-container .content .info-view .price .num {
    color: #fd4c38;
    font-size: 32rpx;
    font-weight: 500;
    padding-bottom: 12rpx;
}

.scenic-list-container .content .info-view .price .num text {
    font-size: 24rpx;
    font-weight: 400;
}

.scenic-list-container .content .info-view .price .msg {
    color: #999;
    font-size: 24rpx;
}

.scenic-list-container .content .info-view .price view {
    align-items: center;
    border-right: 1rpx solid #e4e4e4;
    display: flex;
    flex: 1;
    flex-direction: column;
    padding: 12rpx 0;
}

.scenic-list-container .content .info-view .price view:last-child {
    border-right: none;
}

.scenic-list-container .content .info-view .detail_notice {
    display: flex;
    flex-direction: column;
    line-height: 50rpx;
    margin-top: 24rpx;
}

.scenic-list-container .content .info-view .detail_notice text {
    display: inline-block;
    width: 100%;
}

.scenic-list-container .content .detail {
    background-color: #fff;
    margin-top: 24rpx;
}

.scenic-list-container .content .detail .time_chart {
    padding: 40rpx 40rpx 20rpx;
}

.scenic-list-container .content .detail .time_chart image {
    width: 100%;
}

.scenic-list-container .content .detail .detail_body {
    color: #666;
    line-height: 1.3;
    word-break: break-all;
}

.scenic-list-container .content .header_info {
    align-items: center;
    display: flex;
    justify-content: space-between;
}

.scenic-list-container .content .header_info .head_img {
    background-size: cover;
    border-radius: 27px;
    height: 54px;
    overflow: hidden;
    width: 54px;
}

.scenic-list-container .content .header_info .nick_name {
    padding: 0 30rpx;
}

.scenic-list-container .content .header_info .nick_name .usernickname {
    font-size: 32rpx;
}

.scenic-list-container .content .header_info .nick_name .nickphone {
    font-size: 24rpx;
    margin-top: 20rpx;
}

.scenic-list-container .content .detail_pannel {
    background: #fff;
    border-bottom: 1rpx solid #f2f3f4;
    display: flex;
    justify-content: space-around;
    padding: 30rpx;
    position: sticky;
    text-align: justify;
    top: 0;
    z-index: 10;
}

.scenic-list-container .content .detail_pannel .pannel_title {
    align-items: center;
    display: flex;
    flex-direction: column;
}

.scenic-list-container .content .detail_pannel .pannel_title.active {
    color: #ffe60f;
}

.scenic-list-container .content .detail_pannel .pannel_title.active text {
    border-bottom: 3rpx solid #ffe60f;
    margin-top: 58rpx;
    position: absolute;
    width: 50rpx;
}

.scenic-list-container .share_btn {
    background: #fff;
    display: flex;
    justify-content: flex-end;
}

.scenic-list-container .share_btn:after {
    border: none;
    border-radius: none;
}

.scenic-list-container .footer {
    background: #fff;
    bottom: 0;
    display: flex;
    font-size: 32rpx;
    justify-content: center;
    padding: 20rpx 0 0;
    position: fixed;
    width: 100%;
    z-index: 100;
}

.scenic-list-container .footer .btn_area {
    display: flex;
    margin-bottom: calc(env(safe-area-inset-bottom) + 20rpx);
    width: 50%;
}

.scenic-list-container .footer .btn_area .icon {
    align-items: center;
    background: #fff;
    display: flex;
    flex: 1;
    flex-direction: column;
    font-size: 12px;
    justify-content: center;
    line-height: 1;
    padding: 0;
}

.scenic-list-container .footer .btn_area .icon image {
    height: 48rpx;
    margin-bottom: 10rpx;
    width: 48rpx;
}

.scenic-list-container .footer .btn_area .icon::after {
    border: 0;
}

.scenic-list-container .footer .goyuyue {
    align-items: center;
    background: linear-gradient(90deg,#f79204,#f75b4c);
    border: none;
    box-sizing: border-box;
    color: #fff;
    display: flex;
    font-size: 36rpx;
    justify-content: center;
    line-height: normal;
    margin: 0 12rpx;
    padding: 24rpx 0;
    width: 100%;
}

.scenic-list-container .footer .goyuyue image {
    height: 44rpx;
    width: 48rpx;
}

.scenic-list-container .footer .goyuyue .btnyuyue {
    background: none;
    color: #fff;
    font-size: 32rpx;
    line-height: inherit;
    margin: 0 12rpx 0 24rpx;
    padding: 0;
}

.scenic-list-container .footer .goyuyue:after {
    border: none;
    border-radius: none;
}

.scenic-list-container .footer .yuye_btn_over {
    background: rgba(255,230,15,.5);
}