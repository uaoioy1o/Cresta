.type-button-flex {
    display: -webkit-flex;
    display: flex;
    justify-content: center;
    padding: 0 20rpx;
}

.type-flex-item {
    border-style: solid;
    border-color: #07c160;
    width: 100%;
    height: 80rpx;
    display: flex;
    /*设置显示样式**/
    align-items: center;
    /**子view垂直居中*/
    vertical-align: center;
    /**垂直居中*/
    justify-content: center;
    /**内容居中*/
    flex-direction: row;
}

.type-active {
    background-color: #07c160;
    color: white;
}

.memberBox {
    background-color: #fff;
    border-radius: 10px;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    margin: -15px 30rpx 0;
    padding: 30rpx;
    background-size: cover;
    /* height: 420rpx; */
}