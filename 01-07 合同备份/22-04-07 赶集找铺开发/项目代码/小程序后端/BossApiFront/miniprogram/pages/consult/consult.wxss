.main-title {
    font-size: 32rpx;
}

.pannel .pannel-title {
    font-weight: 500;
    padding: 24rpx;
}

.pannel .pannel-body {
    padding: 24rpx;
}

view {
    color: #333;
    font-size: 28rpx;
    line-height: 1;
}

.right_icon:after {
    background-image: url("https://tystatic.liuxx.com/wechat/images/right_icon.png");
    background-size: cover;
    content: " ";
    display: block;
    height: 20rpx;
    margin-left: 12rpx;
    width: 20rpx;
}

page {
    background-color: #f8f8f8;
}

.mine-feedmsg-box .full-wrap {
    padding: .4rem;
}

.mine-feedmsg-box .full-wrap .content {
    background: #fff;
    border-radius: 5px;
    margin-bottom: .4rem;
    padding: .4rem;
}

.mine-feedmsg-box .full-wrap .content .feed-msg {
    border-radius: 5px;
    overflow: hidden;
    padding: .2rem;
}

.mine-feedmsg-box .full-wrap .content .feed-msg .feedback {
    padding-bottom: .4rem;
}

.mine-feedmsg-box .full-wrap .content .feed-msg .feedback .feedtitle {
    display: inline-block;
    font-size: 32rpx;
    font-weight: 400;
    line-height: 1.5;
    width: 100%;
}

.mine-feedmsg-box .full-wrap .content .feed-msg .feedback text {
    display: inline-block;
    font-size: 24rpx;
    line-height: 1.3;
    width: 100%;
}

.mine-feedmsg-box .full-wrap .content .feed-msg .replay {
    background: #d9f5ff;
    padding: .4rem;
}

.mine-feedmsg-box .full-wrap .content .feed-msg .replay .feedtitle {
    display: inline-block;
    font-size: 32rpx;
    font-weight: 400;
    line-height: 1.5;
    width: 100%;
}

.mine-feedmsg-box .full-wrap .content .feed-msg .replay text {
    display: inline-block;
    font-size: 24rpx;
    line-height: 1.3;
    width: 100%;
}

.mine-feedmsg-box .full-wrap .nomessage {
    color: #666;
    text-align: center;
}